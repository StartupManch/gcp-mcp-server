{
  "name": "gcp-mcp-server",
  "version": "1.0.0",
  "description": "Model Context Protocol (MCP) server for Google Cloud Platform - Talk to your GCP resources",
  "main": "dist/index.js",
  "bin": {
    "gcp-mcp-server": "./bin.js"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "dev:watch": "tsx watch src/index.ts",
    "build": "tsc",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "release": "semantic-release",
    "release:dry": "semantic-release --dry-run",
    "setup-git": "git config commit.template .gitmessage"
  },
  "keywords": [
    "gcp",
    "mcp",
    "claude",
    "ai",
    "llm"
  ],
  "author": "StartupManch Inc. - Devesh Kumar",
  "email": "devesh@startupmanch.in",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/bigquery": "^8.1.1",
    "@google-cloud/billing": "^5.1.0",
    "@google-cloud/billing-budgets": "^6.1.0",
    "@google-cloud/compute": "^6.2.0",
    "@google-cloud/container": "^6.4.0",
    "@google-cloud/functions": "^4.2.0",
    "@google-cloud/logging": "^11.2.0",
    "@google-cloud/resource-manager": "^6.2.0",
    "@google-cloud/run": "^3.0.0",
    "@google-cloud/sql": "^0.21.0",
    "@google-cloud/storage": "^7.17.0",
    "@modelcontextprotocol/sdk": "^1.17.4",
    "google-auth-library": "^10.2.1",
    "googleapis": "^157.0.0",
    "ts-morph": "^26.0.0",
    "tsx": "^4.20.4",
    "zod": "^4.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@jest/globals": "^30.0.5",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.34.0",
    "jest": "^30.0.5",
    "jest-environment-node": "^30.0.5",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.7",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/startupmanch/gcp-mcp-server.git"
  },
  "bugs": {
    "url": "https://github.com/startupmanch/gcp-mcp/issues"
  },
  "homepage": "https://github.com/startupmanch/gcp-mcp#readme"
}
